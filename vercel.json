{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/emailjs",
      "dest": "https://api.emailjs.com/api/v1.0/email/send",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}